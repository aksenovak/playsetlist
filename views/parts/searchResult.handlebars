{{#each setlists}}

    {{#if sets }}
        <div class="set-list-container">
            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                <div class="panel panel-default">
                    <div class="panel-heading" role="tab" id="heading{{"@id"}}">
                        <h4 class="panel-title pull-left">
                            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#setlist{{"@id"}}" aria-expanded="true" aria-controls="{{"@id"}}">
                                {{#if artist}}
                                    <span><strong>Artist:&nbsp;</strong>{{lookup artist "@name" }}</span>
                                {{/if}}
                                <span><b>Date:&nbsp;</b>{{ "@eventDate" }}</span>
                                {{#if '@tour' }}
                                    <span><strong>Tour:&nbsp;</strong>{{ '@tour' }}</span>
                                {{/if}}
                            </a>
                        </h4>
                        <a class="btn btn-danger btn-fab btn-fab-mini pull-right set-list-btn" data-setlist-wrap="#setlist{{"@id"}}"><i class="material-icons">&#xE038;</i></a>
                    </div>
                    <div id="setlist{{"@id"}}" class="panel-collapse collapse out" role="tabpanel" aria-labelledby="heading{{"@id"}}">
                        <div class="panel-body">
                            <ol>
                                {{#if sets.set.length}}
                                    {{#each sets.set}}
                                        {{#if song.length}}
                                            {{#each song }}
                                                <li data-song="{{"@name"}}">{{ "@name" }}</li>
                                            {{/each}}
                                        {{else}}
                                            <li data-song="{{lookup song "@name" }}">{{lookup song "@name" }}</li>
                                        {{/if}}
                                    {{/each}}
                                {{else}}
                                    {{#each sets }}
                                        {{#each song }}
                                            <li data-song="{{"@name"}}">{{ "@name" }}</li>
                                        {{/each}}
                                    {{/each}}
                                {{/if}}
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/if}}

{{/each}}